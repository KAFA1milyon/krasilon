<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>                                
                @Html.ActionLink(" Dashboard ", "Index", "Dashboard", null, new { @class = "navbar-brand" })
                                @Html.ActionLink(" AdresTip ", "Index", "AdresTip", null, new { @class = "navbar-brand" })
                                @Html.ActionLink(" FiyatParametre ", "Index", "FiyatParametre", null, new { @class = "navbar-brand" })
                                @Html.ActionLink(" Musteri ", "Index", "Musteri", null, new { @class = "navbar-brand" })
                                @Html.ActionLink(" MusteriAdres ", "Index", "MusteriAdres", null, new { @class = "navbar-brand" })
                                @Html.ActionLink(" MusteriTelefon ", "Index", "MusteriTelefon", null, new { @class = "navbar-brand" })
                                @Html.ActionLink(" MusteriTip ", "Index", "MusteriTip", null, new { @class = "navbar-brand" })
                                @Html.ActionLink(" OdemeTip ", "Index", "OdemeTip", null, new { @class = "navbar-brand" })
                                @Html.ActionLink(" SatisKanali ", "Index", "SatisKanali", null, new { @class = "navbar-brand" })
                                @Html.ActionLink(" Siparis ", "Index", "Siparis", null, new { @class = "navbar-brand" })
                                @Html.ActionLink(" SiparisOdemeDetay ", "Index", "SiparisOdemeDetay", null, new { @class = "navbar-brand" })
                                @Html.ActionLink(" SiparisUrunDetay ", "Index", "SiparisUrunDetay", null, new { @class = "navbar-brand" })
                                @Html.ActionLink(" TelefonTip ", "Index", "TelefonTip", null, new { @class = "navbar-brand" })
                                @Html.ActionLink(" Urun ", "Index", "Urun", null, new { @class = "navbar-brand" })
                                @Html.ActionLink(" UrunDetay ", "Index", "UrunDetay", null, new { @class = "navbar-brand" })
                                @Html.ActionLink(" UrunFiyat ", "Index", "UrunFiyat", null, new { @class = "navbar-brand" })
                                @Html.ActionLink(" UrunFiyatDetay ", "Index", "UrunFiyatDetay", null, new { @class = "navbar-brand" })
                @Html.ActionLink(" UrunResim ", "Index", "UrunResim", null, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                </ul>
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - by krasilon</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>